    "formik": "^2.0.3",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-confirm-alert": "^2.4.1",
    "react-dom": "^16.12.0",
    "react-js-pagination": "^3.0.2",
    "react-localization": "^1.0.15",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",

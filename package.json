    "react-toastify": "^5.4.1",
    "reactjs-localstorage": "0.0.8",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "sass-loader": "7.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "yup": "^0.27.0"
